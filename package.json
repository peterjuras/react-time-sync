{
  "name": "react-time-sync",
  "version": "2.0.0-alpha.1",
  "description": "A React library to synchronize timers across an application",
  "main": "index.js",
  "repository": "git@github.com:peterjuras/react-time-sync.git",
  "author": "Peter Juras <me@peterjuras.com>",
  "scripts": {
    "build": "rm -rf lib && babel src -d lib --ignore test.js && cp -R scripts .npmignore LICENSE package.json README.md lib",
    "prepublishOnly": "node ./scripts/ensure-publish-path",
    "test": "jest --config .jest.json",
    "test-ci": "jest --config .jest.ci.json && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.0.4",
    "lolex": "^2.1.3",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-test-renderer": "^16.3.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.3.0"
  },
  "dependencies": {
    "time-sync": "^1.0.1"
  }
