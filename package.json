{
  "name": "react-time-sync",
  "version": "3.1.0-alpha2",
  "description": "A React library to synchronize timers across an application",
  "main": "index.js",
  "repository": "git@github.com:peterjuras/react-time-sync.git",
  "author": "Peter Juras <me@peterjuras.com>",
  "scripts": {
    "build": "rm -rf lib && babel src -d lib --ignore test.js && cp -R scripts .npmignore LICENSE package.json README.md lib",
    "prepublishOnly": "node ./scripts/ensure-publish-path",
    "lint": "eslint src/**/*.js",
    "test": "jest --config .jest.json",
    "test-ci": "jest --ci --runInBand --config .jest.ci.json && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "lolex": "^3.0.0",
    "react": "^16.7.0-alpha",
    "react-dom": "^16.7.0-alpha",
    "react-test-renderer": "^16.7.0-alpha"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "time-sync": "^1.2.0"
  }
}
